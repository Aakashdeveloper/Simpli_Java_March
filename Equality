class Flight extends Object{
    private int flightNumber;
    private char flightClass;

    @Override
    public boolean equals(Oject o){
            Flight other = (Flight) o;
            return
            flightNumber == other.flightNumber &&
            flightClass == other.flightClass; 
    }
}

